{
  "name": "cra-boilerplate",
  "version": "0.1.0",
  "description": "Custom create-react-app@next",
  "repository": "transitive-bullshit/cra-boilerplate",
  "license": "MIT",
  "proxy": "http://localhost:5000",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "eject": "react-app-rewired eject",
    "analyze": "source-map-explorer build/static/js/main.*",
    "deploy": "NODE_ENV=production now",
    "force-deploy": "yarn run deploy && now alias && now rm --safe cra-boilerplate",
    "now-start": "serve -s ./build"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "dependencies": {
    "antd": "^3.4.3",
    "axios": "^0.18.0",
    "bowser": "^1.9.3",
    "classnames": "^2.2.5",
    "debug": "^3.1.0",
    "localforage": "^1.7.1",
    "mobx": "^4.2.0",
    "mobx-react": "^5.0.0",
    "prop-types": "^15.6.1",
    "qs": "^6.5.1",
    "react": "^16.3.2",
    "react-app-rewired": "^1.5.0",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "2.0.0-next.66cc7a90",
    "serve": "^6.5.5"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "source-map-explorer": "^1.5.0"
  }
}
